<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhishekyadav.matrixsolver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EnterMatrix" />
        <activity android:name=".EnterMatrixB" />
        <activity android:name=".EnterMatrixC" />
        <activity android:name=".EnterConstantD" />
        <activity android:name=".AddOptions" />
        <activity android:name=".SubOptions" />
        <activity android:name=".MulOptions"></activity>
    </application>

</manifest>